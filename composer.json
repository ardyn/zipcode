{
  "name": "ardan/zipcode",
  "description": "A zip code database for your projects.",
  "authors": [
      {
          "name": "Andrew Ardan",
          "email": "andrew@example.com"
      }
  ],
  "require": {
      "php": ">=5.3.0",
      "illuminate/support": "4.*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.0.*",
    "way/phpunit-wrappers": "dev-master",
    "mockery/mockery": "dev-master",
    "hamcrest/hamcrest-php": "dev-master"
  },
  "autoload": {
      "psr-0": {
          "Ardan\\Zipcode": "src/"
      },
      "classmap": [
        "src/seeds",
        "src/migrations"
      ]
  },
  "minimum-stability": "stable"
}
